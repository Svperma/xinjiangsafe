package com.dsib.entity;

import java.io.Serializable;
import java.util.Date;

/**
 * 荣誉表
 * 
 * @author hslt
 * 
 */
public class GgHonor implements Serializable  {

	private String id;
	private String userCode;// 企业代码
	private String honor;// 荣誉名称
	private Date getDate;// 得奖日期
	private String postUtil;// 颁奖单位
	private String rewardsClassCode;// 颁奖级别
	private Date validityDate;// 有效期
	private String centralizeAddress;// 资料地址
	private String validity;// 0无效，1有效
	private String remark;
	private String flag;

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	public String getUserCode() {
		return userCode;
	}

	public void setUserCode(String userCode) {
		this.userCode = userCode;
	}

	public String getHonor() {
		return honor;
	}

	public void setHonor(String honor) {
		this.honor = honor;
	}

	public Date getGetDate() {
		return getDate;
	}

	public void setGetDate(Date getDate) {
		this.getDate = getDate;
	}

	public String getPostUtil() {
		return postUtil;
	}

	public void setPostUtil(String postUtil) {
		this.postUtil = postUtil;
	}

	public String getRewardsClassCode() {
		return rewardsClassCode;
	}

	public void setRewardsClassCode(String rewardsClassCode) {
		this.rewardsClassCode = rewardsClassCode;
	}

	public Date getValidityDate() {
		return validityDate;
	}

	public void setValidityDate(Date validityDate) {
		this.validityDate = validityDate;
	}

	public String getCentralizeAddress() {
		return centralizeAddress;
	}

	public void setCentralizeAddress(String centralizeAddress) {
		this.centralizeAddress = centralizeAddress;
	}

	public String getValidity() {
		return validity;
	}

	public void setValidity(String validity) {
		this.validity = validity;
	}

	public String getRemark() {
		return remark;
	}

	public void setRemark(String remark) {
		this.remark = remark;
	}

	public String getFlag() {
		return flag;
	}

	public void setFlag(String flag) {
		this.flag = flag;
	}

}
